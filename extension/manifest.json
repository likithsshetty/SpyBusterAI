{
  "manifest_version": 3,
  "name": "Spy Buster AI",
  "version": "1.0",
  "description": "Blocks unsafe websites",
  "permissions": ["tabs", "activeTab", "webRequest", "webNavigation"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
